{
  "author": "Brielle Harrison <nyteshade at gmail.com>",
  "browser": "dist/@nejs/extension.bundle.2.7.1.js",
  "description": "A small library for extending existing JavaScript",
  "devDependencies": {
    "@jest/expect": "^29.7.0",
    "docdash": "^2.0.2",
    "esbuild": "^0.19.10",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-localstorage-mock": "^2.4.26",
    "jsdoc": "^4.0.2",
    "prompts": "^2.4.2",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16"
  },
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "iifeGlobalName": "",
  "jest": {
    "verbose": true,
