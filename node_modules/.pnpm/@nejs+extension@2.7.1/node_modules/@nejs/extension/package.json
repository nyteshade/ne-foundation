  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "name": "@nejs/extension",
  "scripts": {
    "build": "bin/clean && bin/esbuild && bin/build && npm run docs",
    "browser": "bin/esbuild",
    "clean": "bin/clean",
    "docs": "jsdoc -c jsdoc-config.json",
    "module": "bin/build",
    "postpack": "bin/clean && bin/esbuild && bin/build",
    "test": "jest"
  },
  "type": "module",
  "version": "2.7.1"
